  <% dirname = "/home/" %>
  <ol class="breadcrumb">
       <% id = "guest" %>
       <% if current_user != nil %>
         <% id = current_user.netid %>
       <% end %>
       <% dirname = dirname + id + "/" %>
       <li><a href=<%= dirname %>><%= id %></a></li>

       <% dirs = params[:dirs] %>
         <% if dirs != nil %>
         <% dirs.split('/').each do |dir| %>
           <% dirname = dirname + dir + "/" %>
           <li><a href=<%= dirname %>><%= dir %></a></li>
         <% end %>
       <% end %>
  </ol>

  <h3 style="margin-left: 147px">Upload File</h3>
  <input type = "file" name = "file_to_upload" size = 40 style="margin-left: 147px">

  <div class="container">
  <table class="table">
    <thead>
          <tr>
            <th>Name</th>
            <th>Type</th>
            <th>Modified</th>
            <th>Permissions</th>
          </tr>
        </thead>

        <tbody>
          <% getFoldersFromDirectory(params[:dirs]).each do |file| %>
              <tr>
                <% if file.type == "folder" %>
                  <td><a href=<%= dirname + file.name %>><%= file.name -%></a></td>
                <% else %>
                  <td><a href=""><%= file.name -%></a></td>
                <% end %>
                <td><%= file.type -%></td>
                <td><%= file.modified -%></td>
                <td><%= file.permissions -%></td>
              </tr>
          <% end %>
        </tbody>
  </table>
  </div>